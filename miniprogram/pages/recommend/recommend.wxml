<!--pages/recommend/recommend.wxml-->
<view class="container">
	<view class="input-wrap">
		<image mode="widthFix" src="data:image/png;base64,"></image>
		<input placeholder="搜名称试试" confirm-type="搜索"></input>
	</view>

	<image class="adImg" src="https://ntdzwx.dps.qikan.com/Template/WeChatLibrary4/img/defaultBanner.c260fe2c.png" bindtap="onAdTap"></image>

	<view class="category-wrap">
		<view class="category-sub">
			<image src="data:image/png;base64,"></image>
			<view>每日推荐</view>
		</view>
		<view class="category-sub">
			<image src="data:image/png;base64,"></image>
			<view>新书上架</view>
		</view>
		<view class="category-sub">
			<image src="data:image/png;base64,"></image>
			<view>排行榜</view>
		</view>
	</view>

	<x-book-section class="book-section" sectionName="精品期刊" type="magazin" listArr="{{list[0].Data}}" showFooter="{{true}}" bindonTap="onTap"/>
	<x-book-section class="book-section" sectionName="版权图书" type="book" listArr="{{list[1].Data}}" showFooter="{{true}}" bindonTap="onTap"/>
	<x-book-section class="book-section" sectionName="热听音频" type="audio" listArr="{{list[2].Data}}" bindonTap="onTap"/>
	<x-book-section class="book-section" sectionName="报纸" type="newspaper" listArr="{{list[3].Data}}" bindonTap="onTap"/>

</view>

<x-alert-center isShow="{{isShowAlert}}" content-class="alert-content">
	sdioasudoiasudoias
</x-alert-center>
